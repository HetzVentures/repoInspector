{
  "manifest_version": 3,
  "name": "repoInspector",
  "description": "Inspect repositories",
  "version": "1.0.2",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "unlimitedStorage", "activeTab"],
  "options_page": "options.html",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
